{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HeritageApi from \"@/api/HeritageApi\";\nexport default {\n  name: \"HeritageManage\",\n  data() {\n    return {\n      pageSize: 10,\n      pageNum: 1,\n      total: 0,\n      search: '',\n      type: '',\n      fileList: [{\n        name: \"1.png\",\n        url: \"http://localhost:8001/heritageImg/1.png\"\n      }],\n      imgPre: 'http://localhost:8001/heritageImg/',\n      HeritageData: [],\n      rankingData: [],\n      addHeritageDrawer: false,\n      updateHeritageDrawer: false,\n      heritageInfDrawer: false,\n      direction: 'rtl',\n      HeritageForm: {\n        heritageName: '',\n        heritageCity: '',\n        heritageAddress: '',\n        heritageDetail: '',\n        heritageImg: ''\n      }\n    };\n  },\n  methods: {\n    initEcharts() {\n      console.log(\"gg\");\n      let myChart = this.$echarts.init(document.getElementById('heritageChart'));\n      const option = {\n        series: [{\n          type: 'pie',\n          data: [{\n            value: 335,\n            name: '直接访问'\n          }, {\n            value: 234,\n            name: '联盟广告'\n          }, {\n            value: 1548,\n            name: '搜索引擎'\n          }]\n        }]\n      };\n      myChart.setOption(option);\n    },\n    handleRemove(file) {\n      console.log(file.name);\n    },\n    handleBefore(file) {\n      const isLt20M = file.size / 1024 / 1024 / 1024 < 2;\n      if (file.type !== 'image/png' && file.type !== 'image/jpg' && file.type !== 'image/jpeg') {\n        this.$message.error('上传的头像只能是 PNG、JPG、JPEG格式!');\n        return false;\n      }\n      if (!isLt20M) {\n        this.$message.error('上传的头像大小不能超过 20MB!');\n        return false;\n      }\n      return true;\n    },\n    handleSuccess(res) {\n      this.HeritageForm.heritageImg = this.HeritageForm.heritageImg + res.data.heritageImg + '/';\n      console.log(this.HeritageForm.heritageImg);\n    },\n    openInformation(heritage) {\n      this.heritageInfDrawer = true;\n      this.HeritageForm.heritageName = heritage.heritageName;\n      this.HeritageForm.heritageCity = heritage.heritageCity;\n      this.HeritageForm.heritageAddress = heritage.heritageAddress;\n      this.HeritageForm.heritageDetail = heritage.heritageDetail;\n      this.HeritageForm.heritageImg = heritage.heritageImg;\n    },\n    openAdd() {\n      this.addHeritageDrawer = true;\n      this.fileList = '';\n      this.HeritageForm.heritageName = '';\n      this.HeritageForm.heritageCity = '';\n      this.HeritageForm.heritageAddress = '';\n      this.HeritageForm.heritageDetail = '';\n      this.HeritageForm.heritageImg = '';\n    },\n    openUpdate(heritage) {\n      this.updateHeritageDrawer = true;\n      this.HeritageForm.heritageName = heritage.heritageName;\n      this.HeritageForm.heritageCity = heritage.heritageCity;\n      this.HeritageForm.heritageAddress = heritage.heritageAddress;\n      this.HeritageForm.heritageDetail = heritage.heritageDetail;\n      this.HeritageForm.heritageImg = heritage.heritageImg;\n      let nameList = heritage.heritageImg.split('/');\n      for (const key in nameList) {\n        if (nameList[key] !== \"\") {\n          let imgList = {\n            name: nameList[key],\n            url: this.imgPre + nameList[key]\n          };\n          this.fileList.push(imgList);\n        }\n      }\n      console.log(this.fileList);\n    },\n    addHeritage(heritage) {\n      HeritageApi.addHeritage(heritage).then(res => res.data).then(res => {\n        if (res.code === 200) {\n          this.searchHeritage();\n          this.addHeritageDrawer = false;\n          this.$notify({\n            title: '添加非遗信息',\n            message: '添加非遗信息成功',\n            type: 'success',\n            offset: 100\n          });\n        } else {\n          this.$notify({\n            title: '添加非遗信息',\n            message: '添加非遗信息失败',\n            type: 'error',\n            offset: 100\n          });\n        }\n      });\n    },\n    delHeritage(heritage) {\n      HeritageApi.delHeritage(heritage).then(res => res.data).then(res => {\n        if (res.code === 200) {\n          this.searchHeritage();\n          this.$notify({\n            title: '删除用户信息',\n            message: '删除用户信息成功',\n            type: 'success',\n            offset: 100\n          });\n        } else {\n          this.$notify({\n            title: '删除用户信息',\n            message: '删除用户信息失败',\n            type: 'error',\n            offset: 100\n          });\n        }\n      });\n    },\n    updateHeritage(heritage) {\n      HeritageApi.updateHeritage(heritage).then(res => res.data).then(res => {\n        this.searchUser();\n        this.updateUserDrawer = false;\n        if (res.code === 200) {\n          this.$notify({\n            title: '修改非遗信息',\n            message: '修改非遗信息成功',\n            type: 'success',\n            offset: 100\n          });\n        } else {\n          this.$notify({\n            title: '修改用户信息',\n            message: '修改用户信息失败',\n            type: 'error',\n            offset: 100\n          });\n        }\n      });\n    },\n    searchHeritage() {\n      HeritageApi.searchHeritage(this.pageSize, this.pageNum, this.type, this.search).then(res => res.data).then(res => {\n        this.HeritageData = res.data;\n        this.total = res.total;\n      });\n    },\n    handleSizeChange(val) {\n      this.pageSize = val;\n    },\n    handleCurrentChange(val) {\n      this.pageNum = val;\n    },\n    resetData() {\n      this.search = '';\n      this.type = '';\n      this.searchHeritage();\n    }\n  },\n  mounted() {\n    this.initEcharts();\n    this.searchHeritage();\n  }\n};","map":{"version":3,"names":["HeritageApi","name","data","pageSize","pageNum","total","search","type","fileList","url","imgPre","HeritageData","rankingData","addHeritageDrawer","updateHeritageDrawer","heritageInfDrawer","direction","HeritageForm","heritageName","heritageCity","heritageAddress","heritageDetail","heritageImg","methods","initEcharts","console","log","myChart","$echarts","init","document","getElementById","option","series","value","setOption","handleRemove","file","handleBefore","isLt20M","size","$message","error","handleSuccess","res","openInformation","heritage","openAdd","openUpdate","nameList","split","key","imgList","push","addHeritage","then","code","searchHeritage","$notify","title","message","offset","delHeritage","updateHeritage","searchUser","updateUserDrawer","handleSizeChange","val","handleCurrentChange","resetData","mounted"],"sources":["src/components/HeritageManage/HeritageManage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"dataForm\">\r\n      <el-input class=\"input\" v-model=\"search\" placeholder=\"请输入内容\" size=\"small\">\r\n        <el-select v-model=\"type\" slot=\"prepend\" placeholder=\"请选择\">\r\n          <el-option label=\"所属市州\" value=\"所属市州\"></el-option>\r\n          <el-option label=\"非遗名称\" value=\"非遗名称\"></el-option>\r\n        </el-select>\r\n        <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"searchHeritage\"></el-button>\r\n      </el-input>\r\n      <el-button  class=\"btn\"  plain size=\"small\" @click=\"openAdd\">新增非遗</el-button>\r\n      <el-button class=\"btn\" size=\"small\" icon=\"el-icon-refresh\" @click=\"resetData\">重置</el-button>\r\n      <el-table\r\n          class=\"table\"\r\n          :data=\"HeritageData\"\r\n          style=\"width: 100%;height: 400px\"\r\n          max-height=\"400px\">\r\n        <el-table-column\r\n            prop=\"heritageCity\"\r\n            label=\"所属市州\"\r\n            width=\"150\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"heritageName\"\r\n            label=\"非遗名称\"\r\n            width=\"150\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"非遗详情\"\r\n            width=\"150\">\r\n          <template v-slot=\"scope\">\r\n            <el-link type=\"primary\" :underline=\"false\" @click=\"openInformation(scope.row)\">查看<i class=\"el-icon-view el-icon--right\"></i></el-link>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"操作\"\r\n            width=\"150\">\r\n          <template v-slot=\"scope\">\r\n            <el-tooltip content=\"修改非遗信息\" placement=\"top\" effect=\"dark\">\r\n              <el-button icon=\"el-icon-edit\" plain size=\"mini\" @click=\"openUpdate(scope.row)\"></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip content=\"删除非遗信息\" placement=\"top\" effect=\"dark\">\r\n              <el-button type=\"danger\" icon=\"el-icon-delete\" plain size=\"mini\" @click=\"delHeritage(scope.row)\"></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-pagination\r\n          class=\"page\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n\r\n      <el-drawer\r\n          title=\"用户信息详情\"\r\n          :visible.sync=\"heritageInfDrawer\"\r\n          :direction=\"direction\">\r\n        <el-carousel height=\"200px\" style=\"margin: 10px\">\r\n          <el-carousel-item v-for=\"item in fileList\" :key=\"item\">\r\n            <h3 class=\"small\">{{ item.name }}</h3>\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n\r\n        <el-descriptions style=\"margin: 10px\" :column=\"1\" border>\r\n          <el-descriptions-item>\r\n            <template slot=\"label\">\r\n              <i class=\"el-icon-copy-document\"></i>\r\n              非遗名称\r\n            </template>\r\n            {{this.HeritageForm.heritageName}}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template slot=\"label\">\r\n              <i class=\"el-icon-location-information\"></i>\r\n              所属市州\r\n            </template>\r\n            {{this.HeritageForm.heritageCity}}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template slot=\"label\">\r\n              <i class=\"el-icon-guide\"></i>\r\n              非遗地址\r\n            </template>\r\n            {{ this.HeritageForm.heritageAddress }}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template slot=\"label\">\r\n              <i class=\"el-icon-postcard\"></i>\r\n              非遗简介\r\n            </template>\r\n            {{this.HeritageForm.heritageDetail}}\r\n          </el-descriptions-item>\r\n        </el-descriptions>\r\n      </el-drawer>\r\n\r\n      <el-drawer\r\n          title=\"添加非遗信息\"\r\n          :visible.sync=\"addHeritageDrawer\"\r\n          :direction=\"direction\">\r\n        <el-form ref=\"form\" :model=\"HeritageForm\" label-width=\"80px\">\r\n          <el-form-item label=\"非遗名称\">\r\n            <el-input  v-model=\"HeritageForm.heritageName\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"所属市州\">\r\n            <el-input v-model=\"HeritageForm.heritageCity\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"具体地址\">\r\n            <el-input v-model=\"HeritageForm.heritageAddress\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"非遗简介\">\r\n            <el-input type=\"textarea\" v-model=\"HeritageForm.heritageDetail\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"非遗照片\">\r\n            <el-upload\r\n                action=\"http://localhost:8001/heritageSys/uploadHeritageImg\"\r\n                :on-remove=\"handleRemove\"\r\n                :on-success=\"handleSuccess\"\r\n                :before-upload=\"handleBefore\"\r\n                :file-list=\"fileList\"\r\n                list-type=\"picture\">\r\n              <el-tooltip content=\"只上传JPG/PNG/JPEG文件且不超过20MB\" placement=\"top\" effect=\"dark\">\r\n                <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n              </el-tooltip>\r\n            </el-upload>\r\n          </el-form-item>\r\n\r\n          <el-form-item >\r\n            <el-button type=\"primary\" @click=\"addHeritage(HeritageForm)\" plain size=\"small\">立即添加</el-button>\r\n            <el-button size=\"small\">取消</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-drawer>\r\n\r\n      <el-drawer\r\n          title=\"修改非遗信息\"\r\n          :visible.sync=\"updateHeritageDrawer\"\r\n          :direction=\"direction\">\r\n        <el-form ref=\"form\" :model=\"HeritageForm\" label-width=\"80px\">\r\n          <el-form-item label=\"非遗名称\">\r\n            <el-input  v-model=\"HeritageForm.heritageName\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"所属市州\">\r\n            <el-input v-model=\"HeritageForm.heritageCity\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"具体地址\">\r\n            <el-input v-model=\"HeritageForm.heritageAddress\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"非遗简介\">\r\n            <el-input type=\"textarea\" v-model=\"HeritageForm.heritageDetail\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item >\r\n            <el-button type=\"primary\" @click=\"addHeritage\" plain size=\"small\">立即修改</el-button>\r\n            <el-button size=\"small\">取消</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-drawer>\r\n    </div>\r\n    <div class=\"count\">\r\n      <div id=\"heritageChart\" class=\"myChart\"></div>\r\n    </div>\r\n\r\n    <div class=\"ranking\">\r\n      <span>收藏榜</span>\r\n        <el-table\r\n            :data=\"rankingData\"\r\n            stripe\r\n            style=\"width: 100%;font-size: 13px\"\r\n            :cell-style=\"{padding:'5px'}\"\r\n            max-height=\"300px\">\r\n          <el-table-column\r\n              prop=\"heritageName\"\r\n              label=\"名称\"\r\n              width=\"80\">\r\n          </el-table-column>\r\n          <el-table-column\r\n              prop=\"collect\"\r\n              label=\"收藏\"\r\n              width=\"80\">\r\n          </el-table-column>\r\n        </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport HeritageApi from \"@/api/HeritageApi\";\r\n\r\nexport default {\r\n  name: \"HeritageManage\",\r\n  data(){\r\n    return{\r\n      pageSize:10,\r\n      pageNum:1,\r\n      total:0,\r\n      search:'',\r\n      type:'',\r\n      fileList:[\r\n        {name: \"1.png\", url: \"http://localhost:8001/heritageImg/1.png\"}\r\n      ],\r\n      imgPre:'http://localhost:8001/heritageImg/',\r\n      HeritageData:[],\r\n      rankingData:[],\r\n      addHeritageDrawer:false,\r\n      updateHeritageDrawer:false,\r\n      heritageInfDrawer:false,\r\n      direction:'rtl',\r\n      HeritageForm: {\r\n        heritageName:'',\r\n        heritageCity:'',\r\n        heritageAddress:'',\r\n        heritageDetail:'',\r\n        heritageImg:'',\r\n      },\r\n    }\r\n  },\r\n  methods:{\r\n    initEcharts() {\r\n      console.log(\"gg\")\r\n      let myChart = this.$echarts.init(document.getElementById('heritageChart'))\r\n      const option = {\r\n        series: [\r\n          {\r\n            type: 'pie',\r\n            data: [\r\n              {\r\n                value: 335,\r\n                name: '直接访问'\r\n              },\r\n              {\r\n                value: 234,\r\n                name: '联盟广告'\r\n              },\r\n              {\r\n                value: 1548,\r\n                name: '搜索引擎'\r\n              }\r\n            ]\r\n          }\r\n        ]\r\n      };\r\n      myChart.setOption(option);\r\n    },\r\n    handleRemove(file) {\r\n      console.log(file.name);\r\n    },\r\n    handleBefore(file) {\r\n      const isLt20M = file.size / 1024 / 1024 /1024 < 2;\r\n      if (file.type!=='image/png' && file.type!=='image/jpg' && file.type!=='image/jpeg') {\r\n        this.$message.error('上传的头像只能是 PNG、JPG、JPEG格式!');\r\n        return false\r\n      }\r\n      if (!isLt20M) {\r\n        this.$message.error('上传的头像大小不能超过 20MB!');\r\n        return false\r\n      }\r\n      return true;\r\n    },\r\n    handleSuccess(res){\r\n      this.HeritageForm.heritageImg=this.HeritageForm.heritageImg+res.data.heritageImg+'/'\r\n      console.log(this.HeritageForm.heritageImg)\r\n    },\r\n    openInformation(heritage){\r\n      this.heritageInfDrawer=true\r\n      this.HeritageForm.heritageName=heritage.heritageName\r\n      this.HeritageForm.heritageCity=heritage.heritageCity\r\n      this.HeritageForm.heritageAddress=heritage.heritageAddress\r\n      this.HeritageForm.heritageDetail=heritage.heritageDetail\r\n      this.HeritageForm.heritageImg=heritage.heritageImg\r\n    },\r\n    openAdd(){\r\n      this.addHeritageDrawer=true\r\n      this.fileList=''\r\n      this.HeritageForm.heritageName=''\r\n      this.HeritageForm.heritageCity=''\r\n      this.HeritageForm.heritageAddress=''\r\n      this.HeritageForm.heritageDetail=''\r\n      this.HeritageForm.heritageImg=''\r\n    },\r\n    openUpdate(heritage){\r\n      this.updateHeritageDrawer=true\r\n      this.HeritageForm.heritageName=heritage.heritageName\r\n      this.HeritageForm.heritageCity=heritage.heritageCity\r\n      this.HeritageForm.heritageAddress=heritage.heritageAddress\r\n      this.HeritageForm.heritageDetail=heritage.heritageDetail\r\n      this.HeritageForm.heritageImg=heritage.heritageImg\r\n\r\n\r\n      let nameList=heritage.heritageImg.split('/')\r\n      for(const key in nameList) {\r\n        if(nameList[key]!==\"\"){\r\n          let imgList={name:nameList[key],url:this.imgPre+nameList[key]}\r\n          this.fileList.push(imgList)\r\n        }\r\n      }\r\n      console.log(this.fileList)\r\n    },\r\n\r\n    addHeritage(heritage){\r\n      HeritageApi.addHeritage(heritage).then(res=>res.data).then(res=>{\r\n        if (res.code===200){\r\n          this.searchHeritage()\r\n          this.addHeritageDrawer=false\r\n          this.$notify({\r\n            title: '添加非遗信息',\r\n            message: '添加非遗信息成功',\r\n            type:'success',\r\n            offset: 100\r\n          });\r\n        }else {\r\n          this.$notify({\r\n            title: '添加非遗信息',\r\n            message: '添加非遗信息失败',\r\n            type:'error',\r\n            offset: 100\r\n          });\r\n        }\r\n      })\r\n    },\r\n    delHeritage(heritage){\r\n      HeritageApi.delHeritage(heritage).then(res=>res.data).then(res=>{\r\n        if (res.code===200){\r\n          this.searchHeritage()\r\n          this.$notify({\r\n            title: '删除用户信息',\r\n            message: '删除用户信息成功',\r\n            type:'success',\r\n            offset: 100\r\n          });\r\n        }else {\r\n          this.$notify({\r\n            title: '删除用户信息',\r\n            message: '删除用户信息失败',\r\n            type:'error',\r\n            offset: 100\r\n          });\r\n        }\r\n      })\r\n    },\r\n    updateHeritage(heritage){\r\n      HeritageApi.updateHeritage(heritage).then(res=>res.data).then(res=>{\r\n        this.searchUser()\r\n        this.updateUserDrawer=false\r\n        if (res.code===200){\r\n          this.$notify({\r\n            title: '修改非遗信息',\r\n            message: '修改非遗信息成功',\r\n            type:'success',\r\n            offset: 100\r\n          });\r\n        }else {\r\n          this.$notify({\r\n            title: '修改用户信息',\r\n            message: '修改用户信息失败',\r\n            type:'error',\r\n            offset: 100\r\n          });\r\n        }\r\n      })\r\n    },\r\n    searchHeritage(){\r\n      HeritageApi.searchHeritage(this.pageSize,this.pageNum,this.type,this.search).then(res=>res.data).then(res=>{\r\n        this.HeritageData=res.data